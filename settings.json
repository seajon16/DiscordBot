{
  "logging": {
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "standard": {
        "format": "[%(asctime)s] %(levelname)s: %(message)s",
        "datefmt": "%d-%b-%y %H:%M:%S"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "formatter": "standard",
        "level": "DEBUG",
        "stream": "ext://sys.stdout"
      }
    },
    "loggers": {
      "__main__": {
        "handlers": ["console"],
        "propagate": false,
        "level": "INFO"
      },
      "voicecontroller": {
        "handlers": ["console"],
        "propagate": false,
        "level": "INFO"
      },
      "utilities": {
        "handlers": ["console"],
        "propagate": false,
        "level": "INFO"
      }
    }
  },
  "sb_request_file": "sb_requests.txt",
  "token": "[INSERT TOKEN HERE]",
  "vc_timeout_check_interval_secs": 60,
  "vc_timeout_mins": 30
}
